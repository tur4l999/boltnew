{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:github": "vite build --mode production",
    "build:stackblitz": "STACKBLITZ=true vite build",
    "deploy": "./deploy.sh",
    "deploy:gh-pages": "npm run build:github && npx gh-pages -d dist -m 'Deploy to GitHub Pages'",
    "force-deploy": "./scripts/force-deploy.sh",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:github": "vite preview --base /boltnew/",
    "preview:stackblitz": "STACKBLITZ=true vite preview",
    "tokens": "node scripts/generate-tokens.mjs",
    "validate": "node scripts/validate.mjs",
    "handoff": "npm run tokens && node scripts/make-handoff.mjs",
    "dev:inspect": "vite",
    "figma:generate": "node scripts/generate-figma-data.mjs",
    "figma:export": "npm run tokens && npm run figma:generate",
    "figma:simple": "node scripts/export-to-figma.js",
    "design:publish": "tsx scripts/toDesign.ts"
  },
  "dependencies": {
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@figma/plugin-typings": "^1.117.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.18",
    "dotenv": "^17.2.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.4",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
