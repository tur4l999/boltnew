# API Configuration - PRODUCTION
# ⚠️ BU FAYL NÜMUNƏDIR - Real məlumatları .env faylına əlavə edin

# API Base URL
VITE_API_BASE_URL=http://manager.test-domain.co

# API Language
VITE_API_LANG=az

# API Endpoints
VITE_API_SUBJECTS_ENDPOINT=/api/students/subjects/

# Authentication - Basic Auth
VITE_API_AUTH_TYPE=Basic
VITE_API_AUTH_TOKEN=your_base64_encoded_token_here

# CSRF Token
VITE_API_CSRF_TOKEN=your_csrf_token_here

# School/Student Info
VITE_SCHOOL_NAME=DDA

# API Settings
VITE_API_TIMEOUT=15000
VITE_CACHE_DURATION=1800000

# Debug mode
VITE_DEBUG=false
